

public class Main{
  public static void main(String[] args){

   int a = 5;
   int b = 6;


// Method - 1 :

   int t = a;
   a = b;
   b = t;

// Method - 2 :

   a = a + b;
   b = a - b;
   a = a - b;

// Method - 3 :

   a = a * b;
   b = a / b;
   a = a / b;

// Method - 4 :

   a = a ^ b;
   b = a ^ b;
   a = a ^ b;

// Method - 5 :

   b = a + b - (a = b);                                      

  }
}
